            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"templateId\": \"{{templateId}}\",\n  \"templateVersion\": 1,\n  \"vendor\": \"SMARTCOMM\",\n  \"vendorTemplateKey\": \"TEMPLATE_CC_DISCLOSURE_001\",\n  \"isPrimary\": true,\n  \"templateFields\": [\n    {\n      \"fieldName\": \"customerName\",\n      \"fieldType\": \"string\",\n      \"vendorFieldName\": \"Customer.Name\",\n      \"required\": true,\n      \"dataSource\": {\n        \"serviceName\": \"customer-service\",\n        \"serviceUrl\": \"https://api.example.com\",\n        \"endpoint\": \"/customers/{{customerId}}\",\n        \"method\": \"GET\",\n        \"jsonPath\": \"$.name\",\n        \"timeout\": 5000,\n        \"cacheConfig\": {\n          \"enabled\": true,\n          \"ttlSeconds\": 3600,\n          \"cacheKey\": \"customer:{{customerId}}\"\n        }\n      }\n    },\n    {\n      \"fieldName\": \"disclosureCode\",\n      \"fieldType\": \"string\",\n      \"vendorFieldName\": \"Disclosure.Code\",\n      \"required\": true,\n      \"dataSource\": {\n        \"serviceName\": \"pricing-service\",\n        \"serviceUrl\": \"https://api.example.com\",\n        \"endpoint\": \"/prices/{{pricingId}}\",\n        \"method\": \"GET\",\n        \"jsonPath\": \"$.cardholderAgreementsTncCode\",\n        \"timeout\": 5000\n      }\n    }\n  ]\n}"
