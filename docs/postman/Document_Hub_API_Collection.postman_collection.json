            "body": {
              "mode": "raw",
              "raw": "{\n  \"customerId\": \"{{customerId}}\",\n  \"accountId\": [\n    \"{{accountId}}\"\n  ],\n  \"documentTypeCategoryGroup\": [\n    {\n      \"category\": \"Statements\",\n      \"documentTypes\": [\"monthly_statement\"]\n    }\n  ],\n  \"postedFromDate\": 1704067200,\n  \"postedToDate\": 1735689600,\n  \"pageNumber\": 0,\n  \"pageSize\": 20,\n  \"sortOrder\": [\n    {\n      \"orderBy\": \"datePosted\",\n      \"sortBy\": \"desc\"\n    }\n  ]\n}"
